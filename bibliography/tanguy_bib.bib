	doi = {https://doi.org/10.1016/j.quaint.2015.02.013},
	issn = {1040-6182},
	journal = {Quaternary International},
	keywords = {Stable isotope, Hydrograph separation, Shiyi glacier, Tibetan plateau},
	note = {Larger Asian Rivers 8: Impacts from human activities and climate change},
	pages = {262 - 271},
	title = {The stable isotope evolution in Shiyi glacier system during the ablation period in the north of Tibetan Plateau, China},
	url = {http://www.sciencedirect.com/science/article/pii/S1040618215001020},
	volume = {380-381},
