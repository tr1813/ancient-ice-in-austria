	issn = {1040-6182},
	journal = {Quaternary International},
	keywords = {Cave, Stalagmite, Stable isotopes, Holocene climatic conditions, Eastern Adriatic, Croatia},
	pages = {98-106},
	title = {Holocene climatic conditions in the eastern {A}driatic recorded in stalagmites from {S}tra{\v s}na pe{\'c} {C}ave ({C}roatia)},
	url = {https://www.sciencedirect.com/science/article/pii/S1040618218305974},
