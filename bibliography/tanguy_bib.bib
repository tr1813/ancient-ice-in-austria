	journal = {Earth and Planetary Science Letters},
	keywords = {eustacy, sea-level changes, last glacial maximum, Holocene},
	number = {1},
	pages = {327 - 342},
	title = {Refining the eustatic sea-level curve since the Last Glacial Maximum using far- and intermediate-field sites},
	url = {http://www.sciencedirect.com/science/article/pii/S0012821X98001988},
	volume = {163},
