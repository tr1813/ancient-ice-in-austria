	keywords = {Patagonia, ice mass, remote sensing},
	number = {17},
	title = {Ice loss from the {S}outhern {P}atagonian {I}ce {F}ield, {S}outh {A}merica, between 2000 and 2012},
	url = {https://agupubs.onlinelibrary.wiley.com/doi/abs/10.1029/2012GL053136},
