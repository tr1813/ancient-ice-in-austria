	issn = {0169-555X},
	journal = {Geomorphology},
	keywords = {Southern Alps, Tectonics geomorphology, Thrusting, Strike--slip tectonics, Slovenia, Julian Alps},
	number = {4},
	pages = {597 - 604},
	title = {Tectonic geomorphology of the {T}riglav {L}akes {V}alley (easternmost {S}outhern {A}lps, {N}{W} {S}lovenia)},
	url = {http://www.sciencedirect.com/science/article/pii/S0169555X08003577},
	volume = {103},
