@misc{affolter2019sfiw,
	abstract = {The reasons for the early Holocene temperature discrepancy between northern hemispheric model simulations and paleoclimate reconstructions{\textemdash}known as the Holocene temperature conundrum{\textemdash}remain unclear. Using hydrogen isotopes of fluid inclusion water extracted from stalagmites from the Milandre Cave in Switzerland, we established a mid-latitude European mean annual temperature reconstruction for the past 14,000 years. Our Milandre Cave fluid inclusion temperature record (MC-FIT) resembles Greenland and Mediterranean sea surface temperature trends but differs from recent reconstructions obtained from biogenic proxies and climate models. The water isotopes are further synchronized with tropical precipitation records, stressing the Northern Hemisphere signature. Our results support the existence of a European Holocene Thermal Maximum and data model temperature discrepancies. Moreover, data-data comparison reveals a significant latitudinal temperature gradient within Europe. Last, the MC-FIT record suggests that seasonal biases in the proxies are not the primary cause of the Holocene temperature conundrum.},
	author = {St\'{e}phane {Affolter} and Anamaria Diana {H\"{a}uselmann} and Dominik {Fleitmann} and R Lawrence {Edwards} and Hai {Cheng} and Markus C {Leuenberger}},
	date-added = {2021-11-19 15:21:31 +0100},
	date-modified = {2021-11-19 15:21:31 +0100},
	doi = {10.1594/PANGAEA.904949},
	note = {Supplement to: Affolter, S et al. (2019): Central Europe temperature constrained by speleothem fluid inclusion water isotopes over the past 14,000 years. Science Advances, 5(6), eaav3809, https://doi.org/10.1126/sciadv.aav3809},
	publisher = {PANGAEA},
	title = {{Speleothem fluid inclusion water isotopes and inferred temperatures from Milandre Cave, Switzerland}},
	type = {data set},
	url = {https://doi.org/10.1594/PANGAEA.904949},
	year = {2019},
	bdsk-url-1 = {https://doi.org/10.1594/PANGAEA.904949}}

