	date-modified = {2020-03-12 15:53:19 +0100},
	doi = {10.1080/00040851.1997.12003210},
	eprint = {https://www.tandfonline.com/doi/pdf/10.1080/00040851.1997.12003210},
	journal = {Arctic and Alpine Research},
	number = {1},
	pages = {1-12},
	publisher = {Taylor & Francis},
	title = {{R}ecent {G}lacier {V}ariations in the {S}outhern {P}atagonia {I}cefield, {S}outh {A}merica},
	url = {https://www.tandfonline.com/doi/abs/10.1080/00040851.1997.12003210},
	volume = {29},
