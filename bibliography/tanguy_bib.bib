@article{brady1994silicateco2,
	abstract = {A critical uncertainty in models of the global carbon cycle and climate is the combined effect of organic activity, temperature, and atmospheric CO2 on silicate weathering. Here we present new dissolution rates of anorthite and augite which indicate that silicate weathering in organic-rich solutions is not directly affected by soil CO2 but is very sensitive to temperature. Apparently CO2 accelerates silicate weathering indirectly by fertilizing organic activity and the production of corrosive organic acids. The weathering dependencies highlight the ability of silicate weathering to act as a global thermostat and damp out climate change, when used as input in steady-state carbon cycle and climate models.},
	author = {Patrick V. Brady and Susan A. Carroll},
	date-modified = {2019-03-19 10:22:01 +0100},
	doi = {https://doi.org/10.1016/0016-7037(94)90543-6},
