@article{dalziel1982early,
	abstract = {The building of the present Pacific Ocean margin cordillera in S S America, the islands of the N and S Scotia Ridges, and the Antarctic Peninsula was initiated in Middle - Late Jurassic time with the widespread extrusion of calc-alkaline, dominantly silicic, volcanic rocks throughout the Scotia arc region. These volcanic rocks everywhere rest with profound unconformity on a previously deformed and metamorphosed 'basement' complex. This paper is a review of our state of knowledge of this basement, based on the literature and on largely unpublished field observations by the author and his associates. The pre-Middle Jurassic history of the Scotia arc region has an important bearing on the reconstruction of Gondwanaland and on the early history of the SE Pacific Ocean basin, as well as on the evolution of the Scotia arc itself. The petrotectonic assemblage of the basement, considered in relation to the geology of the S Atlantic-Weddell Sea area as a whole, leaves little doubt that the complex represents portions of the fore-arc and main magmatic arc of a pre-Middle Jurassic subduction zone along the Pacific margin of Gondwanaland. Taken with the distribution of undoubtedly Precambrian crystalline rocks, the analysis also suggests that the Scotia arc may be located in an old reentrant in the margin of the supercontinent and thus calls to question the long-assumed original physical continuity of the Andes and the West Antarctic cordillera.-Author},
	address = {Madison, Wis.},
	author = {Dalziel, Ian W. D.},
	booktitle = {Antarctic Geoscience},
	date-added = {2020-02-24 09:13:43 +0100},
	date-modified = {2020-02-24 09:30:58 +0100},
	editor = {C. Craddock},
	journal = {Antarctic Geoscience},
	pages = {111--126},
	publisher = {University of Wisconsin Press},
	title = {The early (pre-Middle Jurassic) history of the {S}cotia {A}rc {R}egion: a review and progress report: review paper ({A}ntarctica).},
	year = {1982}}

