	issn = {1527-1404},
	journal = {Journal of Sedimentary Research},
	number = {6},
	pages = {967--975},
	publisher = {SEPM Society for Sedimentary Research},
	title = {Classification of dolomite rock textures},
	url = {http://jsedres.geoscienceworld.org/content/57/6/967},
